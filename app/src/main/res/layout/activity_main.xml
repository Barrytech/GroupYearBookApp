<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="YearBook"
            tools:layout_editor_absoluteX="181dp"
            tools:layout_editor_absoluteY="24dp" />



        <ImageView
            android:id="@+id/imageView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/_15872fecb59a78e83024dd56351876b"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="-2dp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="245dp"
            tools:layout_editor_absoluteY="667dp">


            <Button
                android:id="@+id/chrisPageB"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Chris"
                tools:layout_editor_absoluteX="174dp"
                tools:layout_editor_absoluteY="601dp" />

            <Button
                android:id="@+id/husnainPage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Husnain"
                tools:layout_editor_absoluteX="292dp"
                tools:layout_editor_absoluteY="601dp" />
            <Button
                android:id="@+id/barryPage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Barry" />


        </LinearLayout>

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>